{
    "manifest_version": 3,
    "name": "POMODORE Timer",
    "version": "1.0",
    "description": "Helps you focus and get things done.",
    "icons": {
        "16": "clock.png",
        "48": "clock.png",
        "128": "clock.png"
    },
    "action": {
        "default_icon": "clock.png",
        "default_title": "Pomodore Timer",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "storage"
    ]
}